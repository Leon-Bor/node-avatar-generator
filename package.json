    "@types/cors": "^2.8.12",
    "@types/express": "4.17.13",
    "@types/jasmine": "3.10.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "~17.0.21",
    "@types/uuid": "^8.3.4",
    "body-parser": "~1.19.2",
    "bootstrap": "5.1.3",
